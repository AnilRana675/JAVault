{
  "name": "javault-cloudflare-worker",
  "version": "1.0.0",
  "description": "JAVault Scraper Worker for Cloudflare",
  "main": "src/index.js",
  "scripts": {
    "build": "echo 'No build step required for Cloudflare Worker'",
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:dev": "wrangler deploy --env development",
    "tail": "wrangler tail",
    "setup-secrets": "npm run setup-secrets:prod",
    "setup-secrets:prod": "echo 'Setting up production secrets...' && wrangler secret put INTERNAL_API_KEY && wrangler secret put BACKEND_URL"
  },
  "devDependencies": {
    "wrangler": "^3.78.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnilRana675/JAVault.git"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "scraper",
    "video",
    "javault"
  ],
  "author": "AnilRana675",
  "license": "MIT"
}
